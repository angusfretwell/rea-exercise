{
  "name": "rea-test",
  "version": "0.1.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors",
    "build": "webpack --progress --colors",
    "test": "node --harmony_proxies node_modules/.bin/jest",
    "lint": "eslint src"
  },
  "jest": {
    "testRegex": "__tests__/.*-test\\.js$",
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/src/__tests__/styleMock.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/src/alt.js",
      "<rootDir>/node_modules/alt/",
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angusfretwell/rea-test.git"
  },
  "author": "Angus Fretwell <angus@inoutput.io> (http://angusfretwell.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angusfretwell/rea-test/issues"
  },
  "homepage": "https://github.com/angusfretwell/rea-test#readme",
  "dependencies": {
    "alt": "^0.18.4",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-airbnb": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^2.0.0",
    "jest-cli": "^13.1.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
